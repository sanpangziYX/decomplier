<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/ysf_grey_pressed" />
            <stroke android:width="0.5dip" android:color="#ffb7d4ed" />
            <corners android:radius="100.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/ysf_white" />
            <stroke android:width="0.5dip" android:color="#ffb7d4ed" />
            <corners android:radius="100.0dip" />
        </shape>
    </item>
</selector>